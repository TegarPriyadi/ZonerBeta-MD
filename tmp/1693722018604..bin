<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" viewBox="-700 -600 2000 2000" width="297" height="419">
    <defs>
        <style>
            .top {
            fill: #f0574b;
            }

            .beard {
            fill: #9b1c20;
            }

            .ringShadow {
            fill: #6a3c00;
            }

            .ring {
            fill: #f69330;
            }

            .shadow {
            fill: rgb(100, 8, 15);
            opacity: 0.5;
            }


            .orbit {
                animation: 1.5s ud infinite ease-in-out;
                --orbit-amount: 15%;
            }

            .orbit g {
                animation: 1.5s lr infinite ease-in-out;
                animation-delay: -0.375s;
                height: 1.6em;
            }

            @keyframes ud {
                0% {
                    transform: translateY(-var(--orbit-amount));
                }
                50% {
                    transform: translateY(var(--orbit-amount));
                }
                100% {
                    transform: translateY(-var(--orbit-amount));
                }
            }

            @keyframes lr {
                0% {
                    transform: translateX(-var(--orbit-amount));
                }
                50% {
                    transform: translateX(var(--orbit-amount));
                }
                100% {
                    transform: translateX(-var(--orbit-amount));
                }
            }
        </style>

        <filter id="blur" x="-50%" y="-50%" width="200%" height="200%" >
            <feGaussianBlur in="SourceGraphic" stdDeviation="3" />
        </filter>

        <filter id="blur2" x="-50%" y="-50%" width="200%" height="200%" >
            <feGaussianBlur in="SourceGraphic" stdDeviation="2" />
        </filter>

        <path id="top" d="M95.44,124.11c32.61,11.87,64.76,6.4,78-25.26A64.85,64.85,0,0,0,51.63,54.57C41.45,87.34,62.84,112.25,95.44,124.11Z" />

    </defs>

    <title>logo</title>

    <g class="orbit">
        <g>
